<div class="container-fluid bg-primary d-flex justify-content-between">
    <a href="/"><img src="{{ asset('build/logo.svg') }}" alt="logo" class="logo-navbar">Wild-Series</a>
    <div>
        {% if app.user %}
            {{ app.user.email }}
            <a class="text-warning" href="{{ path('app_logout') }}">Log Out</a>
        {% else %}
            <a class="log px-3" href="{{ path('app_login') }}">Log In</a>
            <a class="log px-3" href="{{ path('app_register') }}">Register</a>
        {% endif %}
    </div>
</div>
